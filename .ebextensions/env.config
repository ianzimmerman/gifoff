packages:
  yum:
    gcc-c++: []
    gcc-gfortran: []
    python34-devel: []
    libffi-devel: []
    fontconfig: []
    libjpeg-turbo-devel: []
    libpng-devel: []
    freetype: []
    freetype-devel: []
    fontconfig-devel: [] 

container_commands:
    AddGlobalWSGIGroupAccess:
        command: "if ! grep -q 'WSGIApplicationGroup %{GLOBAL}' ../wsgi.conf ; then echo 'WSGIApplicationGroup %{GLOBAL}' >> ../wsgi.conf; fi;"

option_settings:
  "aws:elasticbeanstalk:container:python:staticfiles":
    "/static/": "caudit/static/"
